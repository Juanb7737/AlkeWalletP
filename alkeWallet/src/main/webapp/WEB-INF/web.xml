<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>alkeWallet</display-name>
  <servlet>
	  <servlet-name>s1</servlet-name>
	  <servlet-class>controller.CuentaController</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>s1</servlet-name>
	  <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
	  <servlet-name>s2</servlet-name>
	  <servlet-class>controller.DepositoController</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>s2</servlet-name>
	  <url-pattern>/deposito</url-pattern>
  </servlet-mapping>
  <servlet>
	  <servlet-name>s3</servlet-name>
	  <servlet-class>controller.RetiroController</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>s3</servlet-name>
	  <url-pattern>/retiro</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
</web-app>